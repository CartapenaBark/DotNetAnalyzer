{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Extensions.Hosting": "Information",
      "DotNetAnalyzer.Core.Roslyn": "Debug"
    }
  },
  "WorkspaceManager": {
    "CacheCapacity": 50,
    "CacheExpiration": "00:30:00",
    "MaxConcurrentLoads": 4
  },
  "CompilationCache": {
    "MaxCacheSize": 20,
    "Enabled": true
  }
}
